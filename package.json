{
  "name": "daxfb-calculator",
  "version": "1.0.2",
  "description": "Calculator/Factory Builder for factory management games",
  "main": "index.js",
  "scripts": {
    "dev": "run-s build-data site",
    "build": "run-s type-check build-data build-only",
    "site": "vite site",
    "preview": "vite preview site",
    "build-only": "vite build site",
    "lint": "run-p eslint stylelint type-check",
    "eslint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix",
    "stylelint": "stylelint \"**/*.vue\" --fix",
    "type-check": "tsc --noEmit && vue-tsc --noEmit -p site",
    "build-data": "ts-node build-data.ts",
    "build-data-debug": "node --loader ts-node/esm --inspect-brk build-data.ts"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/doubleaxe/daxfb-calculator"
  },
  "keywords": [
    "game",
    "calculator",
    "factory builder"
  ],
  "engines": {
    "node": ">=18"
  },
  "type": "module",
  "author": "Alexey Usov (dax@xdax.ru, https://t.me/doubleaxe, https://github.com/doubleaxe)",
  "license": "MIT",
  "dependencies": {
    "@mdi/js": "^7.1.96",
    "@vueuse/components": "^9.11.1",
    "@vueuse/core": "^9.11.1",
    "d3-shape": "^3.2.0",
    "javascript-lp-solver": "github:edwardez/jsLPSolver#13d93daeea0bd0b329cc673e72eedcb796201bf0",
    "js-base64": "^3.7.4",
    "pako": "^2.1.0",
    "vue": "^3.2.41",
    "vuetify": "3.1.2"
  },
  "devDependencies": {
    "@types/d3-shape": "^3.1.1",
    "@types/diff": "^5.0.2",
    "@types/node": "^18.11.18",
    "@types/pako": "^2.0.0",
    "@typescript-eslint/eslint-plugin": "^5.48.2",
    "@typescript-eslint/parser": "^5.48.2",
    "@vitejs/plugin-vue": "^3.2.0",
    "@vue/eslint-config-typescript": "^11.0.2",
    "@vue/tsconfig": "^0.1.3",
    "ajv": "^8.12.0",
    "diff": "^5.1.0",
    "eslint": "^8.32.0",
    "eslint-plugin-vue": "^9.9.0",
    "jimp": "^0.16.2",
    "npm-run-all": "^4.1.5",
    "postcss-html": "^1.5.0",
    "rollup-plugin-visualizer": "^5.9.0",
    "sass": "^1.57.1",
    "stylelint": "^14.16.1",
    "stylelint-config-recommended-vue": "^1.4.0",
    "stylelint-config-standard": "^29.0.0",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.4",
    "unplugin-vue-components": "^0.22.12",
    "vite": "^3.2.5",
    "vue-tsc": "^1.0.24"
  }
}
