{
  "name": "@doubleaxe/daxfb-calculator",
  "private": true,
  "version": "2.0.0-alpha.1",
  "description": "Factory planner for management games - core UI lib",
  "keywords": [
    "game",
    "calculator",
    "factory planner"
  ],
  "author": "Alexey Usov (dax@xdax.ru)",
  "homepage": "https://github.com/doubleaxe/daxfb-calculator",
