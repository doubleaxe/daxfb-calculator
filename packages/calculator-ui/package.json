    "@mantine/hooks": "^8.3.1",
    "@phosphor-icons/react": "^2.1.10",
    "@xyflow/react": "^12.8.4",
    "mobx": "^6.13.7",
    "mobx-react-lite": "^4.1.0",
    "pako": "^2.1.0",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "tslib": "^2.8.1"
  },
  "devDependencies": {
    "@pandacss/dev": "^1.3.0",
    "@types/react": "^19.1.13",
    "@types/react-dom": "^19.1.9",
    "@vitejs/plugin-react": "^5.0.2",
    "autoprefixer": "^10.4.21",
    "postcss": "^8.5.6",
    "postcss-preset-mantine": "^1.18.0",
    "rollup-plugin-visualizer": "^6.0.4",
    "typescript": "^5.9.2",
    "vite": "^7.1.5"
