{
  "$schema": "https://json.schemastore.org/tsconfig",
  "extends": "@doubleaxe/typescript-config/web.json",
  "compilerOptions": {
    "isolatedModules": true,
    "noEmit": true,
    "jsx": "react-jsx",
    "importHelpers": true,
    "paths": {
      "#daxfb-gamedata-generated/*": ["./node_modules/@doubleaxe/daxfb-gamedata-generated/src/*"],
      "#daxfb-shared/*": ["./node_modules/@doubleaxe/daxfb-shared/src/*"],
      "#core/*": ["./src/core/*"],
      "#core-ui/*": ["./src/core-ui/*"],
      "#types/*": ["./src/types/*"],
      "#utils/*": ["./src/utils/*"],
      "styled-system/*": ["./generated/styled-system/*"]
    }
  },
  "include": ["src"],
  "references": [{ "path": "./tsconfig.node.json" }]
}
