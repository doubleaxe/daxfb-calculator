{
  "$schema": "https://json.schemastore.org/tsconfig",
  "extends": "@doubleaxe/typescript-config/web.json",
  "compilerOptions": {
    "isolatedModules": true,
    "noEmit": true,
    "jsx": "react-jsx",
    "importHelpers": true,
    "paths": {
      "#core/*": ["./src/*"],
      "#daxfb-shared/*": ["./node_modules/@doubleaxe/daxfb-shared/src/*"]
    }
  },
  "include": ["src"]
}
